{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "scripts/document_start.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "icons": {
      "128": "images/icon-128.png"
   },
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "options_page": "options.html",
   "permissions": [ "tabs", "contextMenus", "storage", "http://*/*", "https://*/*" ],
   "update_url": "http://service.corom.vn/service/update2/crx",
   "version": "1.1.3",
   "web_accessible_resources": [ "css/style.css", "images/banner-logo.png", "css/banner.css" ]
}
